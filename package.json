{
  "name": "unplugin-skypin",
  "version": "1.0.0",
  "description": "Skypin plugin support across all Unplugin platforms.",
  "keywords": [
    "skypack",
    "skypin",
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform"
  ],
  "homepage": "https://github.com/TropicalRaisel/unplugin-skypin#readme",
  "bugs": {
    "url": "https://github.com/TropicalRaisel/unplugin-skypin/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TropicalRaisel/unplugin-skypin.git"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./nuxt": {
      "require": "./dist/nuxt.js",
      "import": "./dist/nuxt.mjs"
    },
    "./types": {
      "require": "./dist/types.js",
      "import": "./dist/types.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "rimraf dist && tsup ./src/*.ts --format cjs,esm --dts && esno scripts/postbuild.ts",
    "dev": "tsup ./src/*.ts --watch src",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "pnpm lint -- --fix",
    "play": "npm -C playground run dev",
    "prepublishOnly": "pnpm build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "jest",
    "test:update": "jest -u"
  },
  "dependencies": {
    "skypin": "^2.0.10",
    "unplugin": "^0.2.7"
  },
  "peerDependencies": {
    "skypin": "^2.0.10",
    "unplugin": "^0.2.7"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.8.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "bumpp": "^7.1.1",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.4.0",
    "esno": "^0.9.1",
    "fast-glob": "^3.2.7",
    "jest": "^27.1.1",
    "nodemon": "^2.0.12",
    "npm-check-updates": "^11.8.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3",
    "ts-jest": "^27.0.5",
    "tsup": "^4.14.0",
    "typescript": "^4.4.2",
    "vite": "^2.5.5",
    "webpack": "^5.52.0"
  }
}
