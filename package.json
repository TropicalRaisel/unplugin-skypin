{
  "name": "@tropicalraisel/unplugin-skypin",
  "version": "4.4.2",
  "description": "Skypack support across all platforms supported by Unplugin!",
  "keywords": [
    "skypack",
    "cdn",
    "unplugin",
    "vite",
    "nuxt",
    "nuxt-module",
    "vue",
    "webpack",
    "rollup",
    "esbuild",
    "skypin"
  ],
  "homepage": "https://github.com/tropicalraisel/unplugin-skypin#readme",
  "bugs": {
    "url": "https://github.com/tropicalraisel/unplugin-skypin/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tropicalraisel/unplugin-skypin.git"
  },
  "license": "MIT",
  "author": {
    "name": "@tropicalraisel",
    "url": "https://github.com/tropicalraisel"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./nuxt": {
      "require": "./dist/nuxt.js",
      "import": "./dist/nuxt.mjs"
    },
    "./types": {
      "require": "./dist/types.js",
      "import": "./dist/types.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint \"{src,scripts,test}/**/*.ts\"",
    "lint:fix": "pnpm lint -- --fix",
    "prepublishOnly": "pnpm build",
    "release": "bumpp --commit --push --tag",
    "start": "esno src/index.ts",
    "test": "pnpm lint && pnpm build && jest",
    "play": "jiti scripts/buildFixtures.ts"
  },
  "dependencies": {
    "@tropicalraisel/skylib": "^1.0.0",
    "unplugin": "^0.2.16"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.9.0",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@symfony/webpack-encore": "^1.6.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.1",
    "@types/semver": "^7.3.8",
    "@types/webpack": "^5.28.0",
    "beautiful-dom": "^1.0.9",
    "bumpp": "^7.1.1",
    "consola": "^2.15.3",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.4.2",
    "esno": "^0.10.0",
    "fast-glob": "^3.2.7",
    "fs-extra": "^10.0.0",
    "jest": "^27.2.2",
    "jiti": "^1.12.3",
    "nodemon": "^2.0.13",
    "npm-check-updates": "^11.8.5",
    "nuxt": "^2.15.8",
    "nuxt-vite": "^0.2.4",
    "postcss": "^8.3.8",
    "rimraf": "^3.0.2",
    "rollup": "^2.57.0",
    "sort-package-json": "^1.52.0",
    "ts-jest": "^27.0.5",
    "tsup": "^5.2.1",
    "typescript": "^4.4.3",
    "vite": "^2.5.10",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.46.0"
  },
  "peerDependencies": {
    "unplugin": "^0.2.16"
  },
  "publishConfig": {
    "access": "public"
  }
}
