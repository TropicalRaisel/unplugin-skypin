{
  "name": "unplugin-skypin",
  "version": "2.0.3",
  "description": "Skypin plugin support across all Unplugin platforms.",
  "keywords": [
    "skypack",
    "skypin",
    "unplugin",
    "vite",
    "nuxt",
    "webpack",
    "rollup",
    "transform"
  ],
  "homepage": "https://github.com/TropicalRaisel/unplugin-skypin#readme",
  "bugs": {
    "url": "https://github.com/TropicalRaisel/unplugin-skypin/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TropicalRaisel/unplugin-skypin.git"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./nuxt": {
      "require": "./dist/nuxt.js",
      "import": "./dist/nuxt.mjs"
    },
    "./types": {
      "require": "./dist/types.js",
      "import": "./dist/types.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint \"{src,scripts,test}/**/*.ts\"",
    "lint:fix": "pnpm lint -- --fix",
    "prepublishOnly": "pnpm build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "pnpm lint && pnpm test:build && jest",
    "test:build": "pnpm build && jiti scripts/buildFixtures.ts"
  },
  "dependencies": {
    "skypin": "^2.0.11",
    "unplugin": "^0.2.9"
  },
  "peerDependencies": {
    "skypin": "^2.0.11",
    "unplugin": "^0.2.9"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.8.2",
    "@skypack/package-check": "^0.2.2",
    "@symfony/webpack-encore": "^1.6.1",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "@types/webpack": "^5.28.0",
    "beautiful-dom": "^1.0.9",
    "bumpp": "^7.1.1",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.4.0",
    "esno": "^0.9.1",
    "fast-glob": "^3.2.7",
    "fs-extra": "^10.0.0",
    "jest": "^27.2.0",
    "jiti": "^1.12.0",
    "nodemon": "^2.0.12",
    "npm-check-updates": "^11.8.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3",
    "ts-jest": "^27.0.5",
    "tsup": "^4.14.0",
    "typescript": "^4.4.3",
    "typescript-logging": "^1.0.0",
    "vite": "^2.5.7",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  }
}
